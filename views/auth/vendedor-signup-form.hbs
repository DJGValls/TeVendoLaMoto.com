<form action="/auth/signup/vendedor" method="post">
    
    <label for="cif">CIF</label>
    <input type="text" name="cif" value="{{userVendedor.cif}}">
    <br>
    <label for="telefono">Telefono de Contacto</label>
    <input type="number" name="telefono" value="{{userVendedor.telefono}}">
    <br>
    <button>Registrar</button>
    <br>
    {{#if this.errorMessage}}
        <p>{{this.errorMessage}}</p>
    {{/if}}
    <p></p>

</form>



